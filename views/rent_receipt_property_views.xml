<odoo>
  <data>
    <record id="view_rent_receipt_property_form" model="ir.ui.view">
      <field name="name">rent.receipt.property.form</field>
      <field name="model">rent.receipt.property</field>
      <field name="arch" type="xml">
        <form string="Rent Receipt Property">
          <sheet>
            <group>
              <field name="name"/>
              <field name="owner_id"
                options="{'no_open': True, 'no_create': True}"
              />
              <field name="street"/>
              <field name="zip"/>
              <field name="city"/>
            </group>
          </sheet>
        </form>
      </field>
    </record>
    <record id="view_rent_receipt_property_list" model="ir.ui.view">
      <field name="name">rent.receipt.property.list</field>
      <field name="model">rent.receipt.property</field>
      <field name="arch" type="xml">
        <list string="Rent Receipt Property List">
          <field name="name"/>
          <field name="owner_id"/>
          <field name="street"/>
          <field name="zip"/>
          <field name="city"/>
        </list>
      </field>
    </record>
    <record id="action_rent_receipt_property" model="ir.actions.act_window">
      <field name="name">Rent Receipt Property</field>
      <field name="res_model">rent.receipt.property</field>
      <field name="view_mode">list,kanban,form</field>
    </record>
  </data>
</odoo>
